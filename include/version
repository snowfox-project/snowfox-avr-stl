#ifndef INCLUDE_SNOWFOX_AVR_STL_VERSION_H_
#define INCLUDE_SNOWFOX_AVR_STL_VERSION_H_

/**************************************************************************************
 * INCLUDE
 **************************************************************************************/

#include <stdint.h>

/**************************************************************************************
 * NAMESPACE
 **************************************************************************************/

namespace snowfox::avrstl
{

/**************************************************************************************
 * CONSTANT
 **************************************************************************************/

struct Version
{
  static constexpr uint32_t Major = 1;
  static constexpr uint32_t Minor = 1;
  static constexpr uint32_t Patch = 0;
};

